{
  "name": "@ujeebu-org/n8n-nodes-ujeebu",
  "version": "0.1.0",
  "description": "n8n community nodes for Ujeebu API - Web scraping, article extraction, and SERP API",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "ujeebu",
    "web-scraping",
    "article-extraction",
    "serp",
    "google-search",
    "screenshot",
    "pdf"
  ],
  "license": "MIT",
  "homepage": "https://ujeebu.com",
  "author": {
    "name": "Ujeebu",
    "email": "support@ujeebu.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ujeebu/ujeebu-n8n.git"
  },
  "engines": {
    "node": ">=18.10"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -- -c .eslintrc.prepublish.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/UjeebuApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Ujeebu/Ujeebu.node.js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "jest": "^29.7.0",
    "n8n-workflow": "^1.30.0",
    "prettier": "^3.2.4",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}

